{
  "extends": "./node_modules/@adonisjs/mrm-preset/_tsconfig",
  "compilerOptions": {
    "module": "Node16",
    "moduleResolution": "Node16",

    "lib": [
      "dom",
      "es2019",
      "es2020.bigint",
      "es2020.string",
      "es2020.symbol.wellknown"
    ],
    "skipLibCheck": true
  },
  "exclude": [
    "build/*",
    "node_modules/*",
    "src/*",
    "test/*"
  ]
}